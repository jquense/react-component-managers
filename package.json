{
  "name": "react-component-managers",
  "version": "3.1.0",
  "description": "react-component-managers React component",
  "main": "lib/index.js",
  "modules": "es/index.js",
  "files": [
    "lib",
    "es"
  ],
  "private": true,
  "scripts": {
    "build": "babel src --out-dir lib && npm run build:esm",
    "build:esm": "BABEL_ENV=esm babel src --out-dir es",
    "test": "jest",
    "tdd": "jest --watch",
    "prepublishOnly": "npm run build",
    "clean": "rm -rf lib",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^22.4.3",
    "jest": "^22.4.3",
    "prop-types": "^15.6.1",
    "spy-on-component": "^1.1.0"
  },
  "peerDependencies": {
    "react": ">=15.3.0",
    "react-dom": ">=15.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.42",
    "@babel/core": "^7.0.0-beta.42",
    "@semantic-release/changelog": "^2.0.1",
    "@semantic-release/git": "^4.0.1",
    "@semantic-release/npm": "^3.2.2",
    "babel-preset-jason": "^4.1.0",
    "eslint-config-jason": "^4.0.0",
    "eslint-plugin-import": "^2.10.0",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "semantic-release": "^15.1.4"
  },
  "author": "Jason Quense",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jquense/react-component-managers.git"
  },
  "keywords": [
    "react-component"
  ],
  "release": {
    "verifyConditions": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      {
        "path": "@semantic-release/git",
        "assets": [
          "package.json"
        ]
      }
    ],
    "prepare": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git"
    ]
  }
}
